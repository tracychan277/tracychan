    "bootstrap": "^4.0.0",
    "devicon-2.2": "^2.2.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-icons": "^2.2.7",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
