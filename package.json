{
  "name": "tracychan",
  "version": "1.0.0",
  "description": "My personal website",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "deploy": "aws s3 sync build/ s3://tracy-chan-code --exclude .DS_Store --cache-control max-age=604800 --delete",
    "test": "jest tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracychan277/tracychan.git"
  },
  "author": "Tracy Chan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tracychan277/tracychan/issues"
  },
  "homepage": "https://github.com/tracychan277/tracychan#readme",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "devicon-2.2": "^2.2.0",
    "html-react-parser": "^0.9.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^2.2.7",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^7.2.6",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.11"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {},
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/tests/mocks/imageMock.js"
    },
    "verbose": true
  }
}
